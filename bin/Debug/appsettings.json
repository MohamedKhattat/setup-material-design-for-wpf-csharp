{
    "UISettings": {
        "PrimaryColor": "#FF6200EE",
        "SecondaryColor": "#FF03DAC5",
        "FontFamily": "Segoe UI",
        "FontSize": 14,
        "DatePickerStartDate": "1997-01-01",
        "Button": {
            "BackgroundColor": "#FF6200EE",
            "TextColor": "White",
            "Padding": "10,5"
        }
    },
    "ValidationRules": {
        "EmailRegex": "^[\\w-\\.]+@([\\w-]+\\.)+[\\w-]{2,4}$",
        "PasswordMinLength": 8,
        "AllowedFileExtensions": [ ".pdf", ".docx", ".xlsx" ]
    },
    "Toolkits": {
        "MaterialDesign": {
            "EnableRippleEffect": true,
            "DefaultTheme": "Light"
        }
    }
}
